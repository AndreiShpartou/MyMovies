  pod 'Kingfisher'
  pod 'Alamofire'
  pod 'lottie-ios'
  pod 'FirebaseAuth', :inhibit_warnings => true
  pod 'FirebaseFirestore', :inhibit_warnings => true
  pod 'Cloudinary', :inhibit_warnings => true
end
